@{
    ViewBag.Title = "Home Page";
    var totalReservations = ViewBag.totalReservations;
    var availableFlatbeds = ViewBag.availableFlatbeds;
    var availableReefers = ViewBag.availableReefers;
    var availableDryVans = ViewBag.availableDryVans;
    var CustomerNames = String.Join(", ", ViewBag.CustomerNames);

    
    
}



<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <p>There are currently @totalReservations total trailers on the yard.</p>
    <p>There are currently @availableDryVans available Dry Vans ready for rent.</p>
    <p>There are currently @availableReefers available Reefers ready for rent.</p>
    <p>There are currently @availableFlatbeds available Flatbeds ready for rent.</p>
    <p>The Customers are @CustomerNames</p>

</div>